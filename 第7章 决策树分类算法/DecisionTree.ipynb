   "source": [
    "from sklearn.datasets import load_iris\n",
    "#从Scikit-Learn库导入决策树模型中的决策树分类算法\n",
    "from sklearn.tree import DecisionTreeClassifier\n",
    "#载入鸢尾花数据集\n",
    "X, y = load_iris(return_X_y=True)\n",
    "#训练模型\n",
    "clf = DecisionTreeClassifier().fit(X, y)\n",
    "#使用模型进行分类预测\n",
    "clf.predict(X)"
   ]
  },
