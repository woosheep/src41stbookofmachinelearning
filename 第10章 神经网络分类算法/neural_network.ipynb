   "source": [
    "from sklearn.datasets import load_iris\n",
    "#从Scikit-Learn库导入神经网络模型中的神经网络分类算法\n",
    "from sklearn.neural_network import MLPClassifier\n",
    "#载入鸢尾花数据集\n",
    "X, y = load_iris(return_X_y=True)\n",
    "#训练模型\n",
    "clf = MLPClassifier().fit(X, y)\n",
    "#使用模型进行分类预测\n",
    "print(clf.predict(X))"
   ]
